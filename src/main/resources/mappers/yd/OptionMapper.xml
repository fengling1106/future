<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
<!--   
    namespace：必须与对应的接口全类名一致  
    id:必须与对应接口的某个对应的方法名一致       
 -->  
 <mapper namespace="com.valen.future.dao.yd.IOptionDAO">

  	<insert id="create" parameterType="com.valen.future.bean.yd.Option">
 		insert into tb_option(region,house,cell,home_number,name,phone_id,info,create_date) values(#{region},#{house},#{cell},#{homeNumber},#{name},#{phoneId},#{info},#{createDate})
 	</insert>
	          
 </mapper>
